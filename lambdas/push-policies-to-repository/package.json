{
  "name": "@aiaa-lambda/push-policies-to-repository",
  "private": true,
  "version": "1.0.0",
  "description": "Pushes generated policies to the specified CodeCommit repository",
  "contributors": [
    "Adil El Kanabi <adilkan@amazon.fr>",
    "Rafał K. Pawłaszek <rpawlasz@amazon.pl>"
  ],
  "license": "MIT",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "fast-compile": "tsc",
    "compile": "eslint . --ext .ts && tsc -b",
    "npm-install:prod": "npm install --production --prefix ./dist",
    "build:prod": "npm run clean && mkdir ./dist && cp package.json ./dist/package.json && npm run compile && npm run npm-install:prod",
    "test": "eslint . --ext .ts && tsc && jest",
    "test-coverage": "jest --coverage",
    "fast-test": "jest",
    "clean": "rimraf dist",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint --fix . --ext .ts"
  },
  "devDependencies": {
    "@tsconfig/node14": "^1.0.3",
    "@types/aws-lambda": "^8.10.114",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.2",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "eslint-plugin-unicorn": "^46.0.0",
    "jest": "^29.5.0",
    "jest-junit": "^16.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@aws-sdk/client-codecommit": "^3.321.1",
    "@aws-sdk/client-s3": "^3.321.1"
  }
}
